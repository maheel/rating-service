{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "root": true,
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "arrow-parens": 0,
    "import/imports-first": 0,
    "global-require": 0,
    "id-length": 0,
    "new-cap": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "generator-star-spacing": 0,
    "require-yield": 0,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "function-paren-newline": 0,
    "prefer-destructuring": 0
  }
}
