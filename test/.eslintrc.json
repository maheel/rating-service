{
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "prefer-arrow-callback": 0,
    "global-require": 0,
    "func-names": 0,
    "no-unused-vars": 0,
    "no-unused-expressions": 0,
    "class-methods-use-this": 0
  }
}
